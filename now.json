{
  "version": 2,
  "name": "ab-stream-background",
  "builds": [
    {
      "src": "background/next.config.js",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "static/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/background/$1"
    }
  ]
}
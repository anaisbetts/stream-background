{
  "version": 2,
  "name": "ab-stream-background",
  "builds": [
    {
      "src": "background/next.config.js",
      "use": "@now/next"
    },
    {
      "src": "api/**/*.ts",
      "use": "@now/node"
    },
    {
      "src": "static/*.png",
      "use": "@now/optipng"
    },
    {
      "src": "static/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(api|api.*)",
      "dest": "/api/src/index.ts"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/background/$1"
    }
  ]
}
